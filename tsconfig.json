{
  "compilerOptions": {
    "module": "ES2015",
    "moduleResolution": "node",
    "target": "ES2015",
    "sourceMap": true,
    "lib": ["dom", "es2015.promise", "es5"],
    
    // 型チェックを厳しくする
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
  }
}